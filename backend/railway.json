{
    "build": {
      "builder": "NIXPACKS",
      "buildCommand": "npm install"
    },
    "deploy": {
      "startCommand": "node index.js",
      "healthcheckPath": "/",
      "healthcheckTimeout": 180,
      "restartPolicyType": "ON_FAILURE",
      "restartPolicyMaxRetries": 10
    }
  }